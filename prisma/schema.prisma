generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Car {
  id              Int       @id @default(autoincrement())
  createdAt       DateTime  @default(now())
  brandName       String
  groupModelName  String
  modelName       String
  year            Int
  price           BigInt
}

model Leasing {
  id              Int     @id @default(autoincrement())
  leasingName     String
  ratesPercentage Int
  term            Int
}

model User {
  id        Int     @id @default(autoincrement())
  username  String  @unique
  name      String
  balance   BigInt
}